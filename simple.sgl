#CONFIG#
SCREEN_HEIGHT = 100
SCREEN_WIDTH = 150
SCREEN_BG = [0 0 0]
SCREEN_CLEAR_DELAY = 0

#FUNCTIONS#
sin(X) = {
    x = parseInt(X*360/6.28)
    print(x)
    x = x%360
    print(x)
    x = x*6.28/360
    -(x-3.14)+((x-3.14)**3/6)-((x-3.14)**5/124)+((x-3.14)**7/5040)
}

#MAIN#
i = 0
clear()
loop {   

    y = sin(i/3)
    draw(i SCREEN_HEIGHT/2+3*y [255 0 255])

    if i == 50 {
        break
    }
    i = i+1
}

